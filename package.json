{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "main": "public/index.html",
  "license": "MIT",
  "bin": {
    "audit-ci": "bin/audit-ci"
  },
  "scripts": {
    "start": "run-p build:watch test:watch",
    "build": "rollup -c --prod",
    "build:watch": "rollup -cw",
    "audit": "audit-ci -l",
    "test": "run-s test:unit testci",
    "testci": "start-server-and-test serve http://localhost:3000 test:e2e",
    "serve": "serve -l 3000 public/",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "cypress": "cypress open",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iraycd/react-typescript-boilerplate.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "pre-push": "yarn test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@semantic-release/changelog": "^3.0.4",
    "@testing-library/cypress": "^5.0.1",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/jest": "^24.0.18",
    "audit-ci": "^2.3.0",
    "colorette": "^1.1.0",
    "cypress": "^3.4.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.3",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.24.2",
    "semantic-release": "^15.13.24",
    "serve": "^11.1.0",
    "start-server-and-test": "^1.10.2",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
